{
  "name": "defdo-theme",
  "type": "dark",
  "tokenColors": [
    {
      "name": "JSON Property Name (key)",
      "scope": [
        "support.type.property-name.json",
        "support.type.property-name.json punctuation",
      ],
      "settings": {
        "foreground": "#BFA6F1"
      }
    },
    {
      "name": "JSON Property Name (value)",
      "scope": [
        "meta.structure.dictionary.json"
      ],
      "settings": {
        "foreground": "#DEDE2F"
      }
    },
    {
      "name": "JSON puntuation",
      "scope": [
        "source.json",
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.array.begin.json",
        "punctuation.definition.dictionary.end.json",
        "punctuation.definition.array.end.json",
        "punctuation.separator.dictionary.key-value.json"
      ],
      "settings": {
        "foreground": "#FEE496"
      }
    },
    {
      "name": "Elixir default",
      "scope": [
        "source.elixir",
      ],
      "settings": {
        "foreground": "#b29ddb"
      }
    },
    {
      "name": "Elixir Module (MyApp)",
      "scope": [
        "entity.name.type.module.elixir",
      ],
      "settings": {
        "foreground": "#C69602"
      }
    },
    {
      "name": "Elixir comments, strings",
      "scope": [
        "comment.documentation.heredoc.elixir",
      ],
      "settings": {
        "foreground": "#f9bb0259"
      }
    },
    {
      "name": "Elixir constants",
      "scope": [
        "constant.language.elixir",
      ],
      "settings": {
        "foreground": "#0291F7"
      }
    },
    {
      "name": "Elixir variables includes __MODULE__, MyModule as pattern matching",
      "scope": [
        "variable.language.elixir",
        "variable.other.constant.elixir"
      ],
      "settings": {
        "foreground": "#9F7AEA"
      }
    },
    {
      "name": "Elixir Keywords (use, import, require)",
      "scope": [
        "keyword.other.special-method.elixir",
      ],
      "settings": {
        "foreground": "#BFA6F1"
      }
    },
    {
      "name": "Elixir functions",
      "scope": [
        "entity.name.function-call.elixir",
        "entity.name.function.elixir"
      ],
      "settings": {
        "foreground": "#bebc2e"
      }
    },
    {
      "name": "Elixir parameter functions",
      "scope": [
        "parameter.variable.function.elixir",
      ],
      "settings": {
        "foreground": "#f9bc02"
      }
    },
    {
      "name": "Elixir strings",
      "scope": [
        "string.quoted.double.heredoc.elixir",
        "string.quoted.double.elixir"
      ],
      "settings": {
        "foreground": "#bca8e9bb"
      }
    },

    {
      "name": "Elixir charlist",
      "scope": [
        "support.function.variable.quoted.single.elixir"
      ],
      "settings": {
        "foreground": "#D6BA65"
      }
    },
    {
      "name": "Elixir operators |> + - = ",
      "scope": [
        "keyword.operator.other.elixir",
        "punctuation.section.scope.elixir",
        "punctuation.section.function.elixir",
        "punctuation.definition.map.elixir",
        "keyword.operator.assignment.elixir",
        "keyword.operator.comparison.elixir",
      ],
      "settings": {
        "foreground": "#9b9927"
      }
    },
    {
      "name": "Elixir integers",
      "scope": [
        "constant.numeric.elixir"
      ],
      "settings": {
        "foreground": "#C69602"
      }
    },
    {
      "name": "Elixir atoms",
      "scope": [
        "constant.language.symbol.elixir",
      ],
      "settings": {
        "foreground": "#9580f5"
      }
    },
    {
      "name": "Elixir Control Keyword (defmodule, do, if, ...)",
      "scope": [
        "keyword.control.module.elixir",
        "keyword.control.elixir",
      ],
      "settings": {
        "foreground": "#f9bc02"
      }
    },
    {
      "name": "Elixir embedded <%= %>",
      "scope": [
        "punctuation.section.embedded.elixir",
      ],
      "settings": {
        "foreground": "#7E5F7E"
      }
    },
    {
      "name": "Elixir embedded",
      "scope": [
        "source.elixir.embedded",
      ],
      "settings": {
        "foreground": "#b29ddb"
      }
    },
    {
      "name": "Elixir embedded #{foo} where foo is the foreground",
      "scope": [
        "source.elixir.embedded.source",
      ],
      "settings": {
        "foreground": "#9F7AEA"
      }
    },
    {
      "name": "HTML tags",
      "scope": [
        "entity.name.tag.html"
      ],
      "settings": {
        "foreground": "#BFA6F1"
      }
    },
    {
      "name": "HTML tags",
      "scope": [
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#d6bfeb"
      }
    },
    {
      "name": "HTML tags <>",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#7E5F7E"
      }
    },
    {
      "name": "HTML tags",
      "scope": [
        "text.html.heex",
      ],
      "settings": {
        "foreground": "#DFD2F8"
      }
    },
    
    

  ],
  "colors": {
    "icon.foreground": "#f9bc02",
    "activityBar.background": "#0f0f0a",
    "activityBar.dropBorder": "#f9bc0214",
    "activityBar.foreground": "#f9bb02fa",
    "activityBar.inactiveForeground": "#f9bb02b6",
    "activityBar.border": "#f9bc0214",
    "activityBarBadge.background": "#f9bb02fa",
    "activityBarBadge.foreground": "#0f0f0a",
    "activityBar.activeBorder": "#f9bc0214",
    "activityBar.activeBackground": "#f9bb0248",
    "activityBar.activeFocusBorder": "#f9bb0296",
    "sideBar.background": "#0f0f0a",
    "minimap.background": "#0f0f0a",
    "editor.background": "#0f0f0a",
    "editor.foreground": "#2f2f29",
    "editorGroupHeader.tabsBackground": "#0f0f0a",
    "editorGroup.dropBackground": "#f9bb021f",
    "editorGroup.border": "#f9bc0214",
    "tab.activeBackground": "#f9bb021e",
    "tab.unfocusedActiveBackground": "#f9bb021f",
    "tab.inactiveBackground": "#0f0f0a",
    "tab.activeForeground": "#f9bc02",
    "editorLineNumber.foreground": "#f9bb024d",
    "editorLineNumber.activeForeground": "#f9bc02",
    "badge.background": "#f9bc02fa",
    "badge.foreground": "#000000",
  }
}