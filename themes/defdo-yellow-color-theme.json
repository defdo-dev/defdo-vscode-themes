{
  "name": "defdo-theme",
  "type": "dark",
  "tokenColors": [
    {
      "name": "JSON Property Name (key)",
      "scope": [
        "support.type.property-name.json",
        "support.type.property-name.json punctuation",
      ],
      "settings": {
        "foreground": "#BFA6F1"
      }
    },
    {
      "name": "JSON Property Name (value)",
      "scope": [
        "meta.structure.dictionary.json"
      ],
      "settings": {
        "foreground": "#DEDE2F"
      }
    },
    {
      "name": "JSON puntuation",
      "scope": [
        "source.json",
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.array.begin.json",
        "punctuation.definition.dictionary.end.json",
        "punctuation.definition.array.end.json",
        "punctuation.separator.dictionary.key-value.json"
      ],
      "settings": {
        "foreground": "#FEE496"
      }
    },
    {
      "name": "Elixir default",
      "scope": [
        "source.elixir",
      ],
      "settings": {
        "foreground": "#b29ddb"
      }
    },
    {
      "name": "Elixir Module (MyApp)",
      "scope": [
        "entity.name.type.module.elixir",
      ],
      "settings": {
        "foreground": "#C69602"
      }
    },
    {
      "name": "Elixir comments, strings",
      "scope": [
        "comment.documentation.heredoc.elixir",
      ],
      "settings": {
        "foreground": "#f9bb0259"
      }
    },
    {
      "name": "Elixir constants",
      "scope": [
        "constant.language.elixir",
      ],
      "settings": {
        "foreground": "#0291F7"
      }
    },
    {
      "name": "Elixir variables includes __MODULE__, MyModule as pattern matching",
      "scope": [
        "variable.language.elixir",
        "variable.other.constant.elixir"
      ],
      "settings": {
        "foreground": "#9F7AEA"
      }
    },
    {
      "name": "Elixir Keywords (use, import, require)",
      "scope": [
        "keyword.other.special-method.elixir",
      ],
      "settings": {
        "foreground": "#BFA6F1"
      }
    },
    {
      "name": "Elixir functions",
      "scope": [
        "entity.name.function-call.elixir",
        "entity.name.function.elixir"
      ],
      "settings": {
        "foreground": "#bebc2e"
      }
    },
    {
      "name": "Elixir parameter functions",
      "scope": [
        "parameter.variable.function.elixir",
      ],
      "settings": {
        "foreground": "#f9bc02"
      }
    },
    {
      "name": "Elixir strings",
      "scope": [
        "string.quoted.double.heredoc.elixir",
        "string.quoted.double.elixir"
      ],
      "settings": {
        "foreground": "#bca8e9bb"
      }
    },

    {
      "name": "Elixir charlist",
      "scope": [
        "support.function.variable.quoted.single.elixir"
      ],
      "settings": {
        "foreground": "#D6BA65"
      }
    },
    {
      "name": "Elixir operators |> + - = ",
      "scope": [
        "keyword.operator.other.elixir",
        "punctuation.section.scope.elixir",
        "punctuation.section.function.elixir",
        "punctuation.definition.map.elixir",
        "keyword.operator.assignment.elixir",
        "keyword.operator.comparison.elixir",
      ],
      "settings": {
        "foreground": "#9b9927"
      }
    },
    {
      "name": "Elixir integers",
      "scope": [
        "constant.numeric.elixir"
      ],
      "settings": {
        "foreground": "#C69602"
      }
    },
    {
      "name": "Elixir atoms",
      "scope": [
        "constant.language.symbol.elixir",
      ],
      "settings": {
        "foreground": "#9580f5"
      }
    },
    {
      "name": "Elixir Control Keyword (defmodule, do, if, ...)",
      "scope": [
        "keyword.control.module.elixir",
        "keyword.control.elixir",
      ],
      "settings": {
        "foreground": "#f9bc02"
      }
    },
    {
      "name": "Elixir embedded <%= %>",
      "scope": [
        "punctuation.section.embedded.elixir",
      ],
      "settings": {
        "foreground": "#7E5F7E"
      }
    },
    {
      "name": "Elixir embedded",
      "scope": [
        "text.html.heex",
      ],
      "settings": {
        "foreground": "#BEBC2E"
      }
    },
    {
      "name": "Elixir embedded #{foo} where foo is the foreground",
      "scope": [
        "source.elixir.embedded.source",
      ],
      "settings": {
        "foreground": "#9F7AEA"
      }
    },
    {
      "name": "HTML tags",
      "scope": [
        "entity.name.tag.html"
      ],
      "settings": {
        "foreground": "#BFA6F1"
      }
    },
    {
      "name": "HTML tags",
      "scope": [
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#d6bfeb"
      }
    },
    {
      "name": "HTML tags <>",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#7E5F7E"
      }
    },
    {
      "name": "HTML tags",
      "scope": [
        "text.html.heex",
      ],
      "settings": {
        "foreground": "#DFD2F8"
      }
    },
    {
      "name": "Markdown heading h1",
      "scope": [
        "markup.heading.setext.1.markdown",
        "heading.1.markdown",
      ],
      "settings": {
        "foreground": "#BEBC2E"
      }
    },
    {
      "name": "Markdown heading h2, h3",
      "scope": [
        "markup.heading.setext.2.markdown",
        "markup.heading.setext.3.markdown",
        "heading.2.markdown",
        "heading.3.markdown",
      ],
      "settings": {
        "foreground": "#cac82c"
      }
    },
    {
      "name": "Markdown list",
      "scope": [
        "markup.list.unnumbered.markdown",
        "punctuation.definition.list.begin.markdown",
      ],
      "settings": {
        "foreground": "#d6bfeb"
      }
    },
    {
      "name": "Markdown link",
      "scope": [
        "string.other.link.title.markdow",
        "string.other.link.description.markdown",
      ],
      "settings": {
        "foreground": "#f9bc02"
      }
    },
    {
      "name": "Markdown link",
      "scope": [
        "meta.link.inline.markdown",
        "markup.underline.link.image.markdown",
      ],
      "settings": {
        "foreground": "#f8e19b"
      }
    },
    {
      "name": "Markdown `string` > ~~~ --- ^ ! ***",
      "scope": [
        "punctuation.definition.raw.markdown",
        "markup.inline.raw.string.markdown",
        "markup.raw.block.markdown",
        "punctuation.definition.quote.begin.markdown",
        "meta.separator.markdown",
        "markup.math.inline.markdown",
        "punctuation.math.operator.latex",
        "punctuation.math.begin.bracket.curly",
        "punctuation.definition.string.begin.markdown",
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "#BEBC2E"
      }
    },
    {
      "name": "Markdown italic",
      "scope": [
        "markup.italic.markdown",
        "markup.bold.markdown",
      ],
      "settings": {
        "foreground": "#9580f5"
      }
    },
    {
      "name": "Markdown language",
      "scope": [
        "meta.embedded.block.python",
        "fenced_code.block.language.markdown",
        "markup.fenced_code.block.markdown",
      ],
      "settings": {
        "foreground": "#9580f5"
      }
    },
    
    

  ],
